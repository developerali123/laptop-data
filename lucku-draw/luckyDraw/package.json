{
  "name": "electron-express-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-packager . LuckyDrawFinalTest --platform=win32 --arch=x64 --out=dist/ --overwrite"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "electron": "^38.4.0",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "backend/**"
    ],
    "files": [
      "main.js",
      "preload.js",
      "renderer/**/*",
      "backend/**/*",
      "package.json"
    ]
  }
}
